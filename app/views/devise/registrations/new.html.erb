
<body id="start">
    
<h2>Sign up</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
 <p> <%= devise_error_messages! %></p>

  <div><p><%= f.label :email %></p>
  <%= f.email_field :email, :autofocus => true %></div>
<br />
  <div><p><%= f.label :password %></p>
  <%= f.password_field :password %></div>
<br />
  <div><p><%= f.label :password_confirmation %></p>
  <p><%= f.password_field :password_confirmation %></p></div>

<div> <p> <%= f.submit "Sign up" %></p></div>
<% end %>

<p><%= render "devise/shared/links" %></p>



